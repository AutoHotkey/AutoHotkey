FONT 10, "Segoe UI", 400, 0, 0x0
BEGIN
    EDITTEXT        IDC_INPUTEDIT,2,51,207,12,ES_AUTOHSCROLL
    DEFPUSHBUTTON   "OK",IDOK,51,67,50,12
    PUSHBUTTON      "Cancel",IDCANCEL,129,67,50,12
